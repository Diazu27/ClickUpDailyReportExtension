{
    "name": "ClickUp Daily Report",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "A chromium extension to create a ClickUp daily report to text",
    "action":{
        "default_popup": "index.html"
    },
    "permissions": ["tabs", "activeTab","storage","scripting"],
    "host_permissions": ["https://*/*"],
    "icons": {
        "16": "assets/icons/nasapod16x16.png",
        "32": "assets/icons/nasapod32x32.png",
        "48": "assets/icons/nasapod48x48.png",
        "128":"assets/icons/nasapod128x128.png"
    }
  }
  